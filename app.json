{
  "expo": {
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icons/android/play_store_512.png",
        "monochromeImage": "./assets/images/icons/android/res/mipmap-hdpi/ic_launcher_monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tach.pitchme",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "predictiveBackGestureEnabled": false
    },
    "experiments": {
      "reactCompiler": true,
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "21ed52dd-d769-4576-98d2-4ef86bd5443c"
      },
      "router": {}
    },
    "icon": "./assets/images/icons/ios/AppIcon~ios-marketing.png",
    "ios": {
      "icon": "./assets/images/icons/ios/AppIcon~ios-marketing.png",
      "supportsTablet": true
    },
    "name": "pitchme",
    "newArchEnabled": true,
    "orientation": "portrait",
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          },
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "cover"
        }
      ],
      "expo-audio",
      "expo-web-browser",
      "expo-asset"
    ],
    //chuiw
    "scheme": "pitchme",
    "slug": "pitchme",
    "userInterfaceStyle": "automatic",
    "version": "1.0.6",
    "web": {
      "favicon": "./assets/images/icon.png",
      "output": "static"
    }
  }
}